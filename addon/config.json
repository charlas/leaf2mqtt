{
  "name": "Leaf2MQTT",
  "version": "46apc",
  "slug": "leaf2mqtt",
  "description": "Interact with your Nissan Leaf using MQTT",
  "startup": "application",
  "url": "https://github.com/yp87/leaf2mqtt",
  "init": false,
  "arch": [
    "aarch64",
    "amd64"
  ],
  "options": {
    "LEAF_USERNAME": "",
    "LEAF_PASSWORD": "",
    "LEAF_TYPE": "newerThanMay2019",
    "MQTT_HOST": "homeassistant.local",
    "MQTT_PORT": "1883",
    "MQTT_USERNAME": "mqtt",
    "MQTT_PASSWORD": "",
    "MQTT_BASE_TOPIC": "nissanleaf",
    "UPDATE_INTERVAL_MINUTES": "45",
    "CHARGING_UPDATE_INTERVAL_MINUTES": "30",
    "COMMAND_ATTEMPTS": "5",
    "LOG_LEVEL": ""
  },
  "schema": {
    "LEAF_USERNAME": "str",
    "LEAF_PASSWORD": "password",
    "LEAF_TYPE": "list(newerThanMay2019|olderCanada|olderUSA|olderEurope|olderAustralia|olderJapan)",
    "MQTT_HOST": "str",
    "MQTT_PORT": "int?",
    "MQTT_USERNAME": "str?",
    "MQTT_PASSWORD": "password?",
    "MQTT_BASE_TOPIC": "str?",
    "UPDATE_INTERVAL_MINUTES": "int(5,)?",
    "CHARGING_UPDATE_INTERVAL_MINUTES": "int(5,)?",
    "COMMAND_ATTEMPTS": "int(1,)?",
    "LOG_LEVEL": "list(All|Info|Warning|Severe)"
  }
}
